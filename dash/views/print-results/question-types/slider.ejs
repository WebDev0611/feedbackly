  <% questionData.totals.forEach(function(total) { %>
    <div class="chip result-total" style="border-right: 3px solid <%= (questionData.colorMap || {})[total.name.toString()] %>">
      <%= total.name %> <%= total.average %> (<%= total.value %>)
